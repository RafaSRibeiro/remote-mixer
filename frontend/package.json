  "license": "MIT",
  "private": true,
  "scripts": {
    "analyze": "webpack --config ./webpack.config.ts --env production --env analyze",
    "build:base": "cross-env NODE_OPTIONS=--no-experimental-strip-types webpack --config ./webpack.config.ts",
    "build": "cross-env NODE_ENV=production yarn build:base --env production",
    "build:dev": "yarn build:base",
    "build:profile": "yarn build:base --env profile",
    "build:profileReact": "yarn build:base --env profileReact",
    "clean": "rimraf dist",
    "lint:css": "stylelint '**/*.@(ts|tsx)'",
    "dev": "cross-env NODE_OPTIONS=--no-experimental-strip-types webpack serve --config ./webpack.config.ts",
    "tsc:watch": "tsc -b --watch"
  },
  "dependencies": {
    "@remote-mixer/controls": "*",
    "@remote-mixer/types": "*",
    "@remote-mixer/utils": "*"
  },
