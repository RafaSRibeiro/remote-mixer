    "@wyw-in-js/babel-preset": "^0.7.0",
    "@wyw-in-js/webpack-loader": "^0.7.0",
    "babel-loader": "^10.0.0",
    "cnbuilder": "^3.1.0",
    "core-js": "^3.44.0",
    "cross-env": "^10.0.0",
    "css-loader": "^7.1.2",
