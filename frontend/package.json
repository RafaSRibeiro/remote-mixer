    "@babel/preset-typescript": "^7.27.1",
    "@babel/runtime": "^7.28.2",
    "@linaria/core": "^6.3.0",
    "@linaria/postcss-linaria": "^6.3.0",
    "@linaria/react": "^6.3.0",
    "@mdi/js": "^7.4.47",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.1",
